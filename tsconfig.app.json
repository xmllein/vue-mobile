{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "mock/**/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "types": [
      "node" 
    ],
    "noImplicitAny": false,
    "allowJs": true,
    "baseUrl": ".",
